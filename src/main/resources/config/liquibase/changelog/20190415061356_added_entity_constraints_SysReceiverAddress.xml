<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity SysReceiverAddress.
    -->
    <changeSet id="20190415061356-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="sys_receiver_address"
                                 constraintName="fk_sys_receiver_address_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sys_user"/>

        <addForeignKeyConstraint baseColumnNames="sys_receiver_addresses_id"
                                 baseTableName="sys_receiver_address_product"
                                 constraintName="fk_sys_receiver_address_product_sys_receiver_addresses_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sys_receiver_address"/>
        <addForeignKeyConstraint baseColumnNames="products_id"
                                 baseTableName="sys_receiver_address_product"
                                 constraintName="fk_sys_receiver_address_product_products_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sys_order"/>
        
    </changeSet>
</databaseChangeLog>
